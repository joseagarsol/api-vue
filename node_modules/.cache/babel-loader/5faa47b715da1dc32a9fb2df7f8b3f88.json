AACA,MAAA,IAAA,CAAA;AACA,QAAA,KAAA,EAAA,eADA;AAEA,QAAA,IAAA,EACA,oEAHA;AAIA,QAAA,IAAA,EAAA,SAJA;AAKA,QAAA,OAAA,EAAA,IALA;AAMA,QAAA,UAAA,EAAA;AANA,OAAA,CAAA,CAOA,IAPA,CAOA,UAAA,UAAA,EAAA;AACA,YAAA,UAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,GAAA,GAAA,UAAA,GAAA,SAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,IAAA,CACA,kBADA,EAEA,yCAFA,EAGA,SAHA,CAAA;AAKA,YAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA,WARA;AASA,SAVA,MAUA;AACA,UAAA,IAAA,CAAA,mCAAA,CAAA;AACA;AACA,OArBA;AAsBA;AA/BA;AAfA,CAAA","sourcesContent":["<template>\r\n  <div class=\"general\">\r\n    <div class=\"center\">\r\n      <section id=\"content\">\r\n        <article class=\"article-item article-detail\" v-if=\"article\">\r\n          <div class=\"image-wrap\" v-if=\"article.image\">\r\n            <img\r\n              :src=\"url + 'get-image/' + article.image\"\r\n              :alt=\"article.title\"\r\n            />\r\n          </div>\r\n\r\n          <h1 class=\"subheader\">{{ article.title }}</h1>\r\n          <span class=\"date\">{{ article.date | moment(\"from\", \"now\") }}</span>\r\n          <p>\r\n            {{ article.content }}\r\n          </p>\r\n\r\n          <div class=\"clearfix\"></div>\r\n\r\n          <router-link :to=\"'/editar/' + article._id\" class=\"btn btn-warning\"\r\n            >Editar</router-link\r\n          >\r\n          <a @click=\"deleteArticle(article._id)\" class=\"btn btn-danger\"\r\n            >Eliminar</a\r\n          >\r\n        </article>\r\n      </section>\r\n      <Sidebar />\r\n      <div class=\"clearfix\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Sidebar from \"./Sidebar.vue\";\r\nimport Global from \"../global\";\r\nimport axios from \"axios\";\r\nimport swal from \"sweetalert\";\r\n\r\nexport default {\r\n  name: \"Article\",\r\n  components: {\r\n    Sidebar,\r\n  },\r\n  data() {\r\n    return {\r\n      url: Global.url,\r\n      article: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    var articleId = this.$route.params.id;\r\n    this.getArticle(articleId);\r\n  },\r\n  methods: {\r\n    getArticle(articleId) {\r\n      axios.get(this.url + \"article/\" + articleId).then((res) => {\r\n        if (res.data.status == \"success\") {\r\n          this.article = res.data.article;\r\n        }\r\n      });\r\n    },\r\n    deleteArticle(articleId) {\r\n      swal({\r\n        title: \"Are you sure?\",\r\n        text:\r\n          \"Once deleted, you will not be able to recover this imaginary file!\",\r\n        icon: \"warning\",\r\n        buttons: true,\r\n        dangerMode: true,\r\n      }).then((willDelete) => {\r\n        if (willDelete) {\r\n          axios.delete(this.url + \"article/\" + articleId).then((response) => {\r\n            swal(\r\n              \"Artículo Borrado\",\r\n              \"El artículo se ha borrado correctamente\",\r\n              \"success\"\r\n            );\r\n            console.log(response.data);\r\n            this.$router.push(\"/blog\");\r\n          });\r\n        } else {\r\n          swal(\"Uff casi!!! No has borrado nada!!\");\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}